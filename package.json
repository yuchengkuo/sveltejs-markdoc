{
  "name": "sveltejs-markdoc",
  "version": "0.2.3",
  "description": "Markdoc renderer for Svelte",
  "packageManager": "pnpm@7.12.2",
  "keywords": [
    "svelte",
    "markdoc",
    "markdown"
  ],
  "license": "MIT",
  "author": {
    "name": "YuCheng Kuo",
    "email": "hey@yuchengkuo.com",
    "url": "https://yuchengkuo.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchengkuo/sveltejs-markdoc"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@markdoc/markdoc": "^0.2.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "1.0.0-next.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitest/coverage-c8": "^0.25.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.20",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.2"
  },
  "type": "module"
}
